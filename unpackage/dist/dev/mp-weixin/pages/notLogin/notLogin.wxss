.notlogin {
  position: relative;
}
.notlogin .notloginimage {
  width: 100%;
  height: 79px;
}
.notlogin .notloginimage image {
  position: absolute;
  top: 32px;
  left: 0;
  width: 100%;
  height: 79px;
}
.notlogin .notloginimage .header {
  position: relative;
  box-sizing: border-box;
}
.notlogin .notloginimage .header .btn {
  border: 0;
  position: absolute;
  top: 40px;
  left: 40px;
  width: 100px;
  height: 30px;
  line-height: 30px;
  text-align: center;
  color: #0e0e0e;
  background-color: #fff7cd;
  font-size: 14px;
  font-weight: 700;
  border-radius: 50px;
}
.notlogin .notloginimage .header .header-right {
  position: absolute;
  top: 29px;
  right: 20px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  z-index: 2;
}
.notlogin .notloginimage .header .header-right .header-right_info,
.notlogin .notloginimage .header .header-right .header-right_info1,
.notlogin .notloginimage .header .header-right .header-right_info2 {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  margin: 0 5px;
  width: 40px;
  height: 43px;
  line-height: 1.8;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  font-size: 14px;
  text-align: center;
}
.notlogin .notloginimage .header .header-right .header-right_info1 {
  font-size: 24px;
  line-height: 43px;
  color: #fde468;
}
.notlogin .notloginimage .header .header-right .header-right_info2 {
  width: 60px;
}
.notlogin .nologin-content {
  margin-top: 30px;
}
.notlogin .nologin-content .nologin-content_list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.notlogin .nologin-content .nologin-content_list .nologin-content_list-title {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  margin: 20px 0;
}
.notlogin .nologin-content .nologin-content_list .nologin-content_list-title .nologin-content_icon {
  margin-right: 5px;
}
.notlogin .nologin-content .nologin-content_list .nologin-content_list-title text {
  font-size: 17px;
}
.notlogin .nologin-content .nologin-content_list .icons,
.notlogin .nologin-content .nologin-content_list text {
  margin-right: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 17px;
}

